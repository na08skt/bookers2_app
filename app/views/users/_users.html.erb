<h3>Users</h3>
    <table>
      <thead>
        <tr>
          <th>Image</th>
          <th>User</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <% @users.each do |user| %>
            <td>
              <%= image_tag user.get_profile_image(100, 100) %>
            </td>
            <td>
              <%= user.name %>
            </td>
            <td>
              <%= link_to "Show", user_path(user.id) %>
            </td>
          <% end %>
        </tr>
      </tbody>
    </table>

